{
    "name": "spice",
    "displayName": "SPICE",
    "description": "SPICE language support",
    "version": "0.0.4",
    "icon": "icon.jpg",
    "publisher": "xuanli",
    "homepage": "https://github.com/lxTHU/vscode-spice",
	"repository": {
        "type": "git",      
        "url": "https://github.com/lxTHU/vscode-spice.git"
    },

	"bugs": {
        "url": "https://github.com/lxTHU/vscode-spice/issues"
    },
    "engines": {
        "vscode": "^1.15.0"
    },
    "categories": [
        "Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "spice",
            "aliases": ["SPICE", "spice"],
            "extensions": [".sp",".net",".scs",".ckt",".lib",".mdl",".cir",".sub",".mod"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "spice",
            "scopeName": "source.sp",
            "path": "./syntaxes/SPICE.tmLanguage"
        }]

    }
}